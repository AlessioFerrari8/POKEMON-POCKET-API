{
  "buildCommand": "cd pokemon-manager && npm install --legacy-peer-deps && node scripts/build-config.js && npm run build",
  "outputDirectory": "pokemon-manager/dist/pokemon-manager/browser",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin-allow-popups"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    }
  ]
}